{
  "data_asset_type": "Table",
  "expectation_suite_name": "iris_silver_suite",
  "expectations": [
    {
      "expectation_type": "expect_table_row_count_to_be_between",
      "kwargs": {
        "min_value": 100,
        "max_value": 200
      },
      "meta": {
        "notes": "Após limpeza, deve manter a maioria dos registros"
      }
    },
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": [
          "sepal_length",
          "sepal_width",
          "petal_length", 
          "petal_width",
          "species"
        ]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "sepal_length"
      },
      "meta": {
        "notes": "Silver layer não deve ter nulos - dados limpos"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "sepal_width"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "petal_length"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "petal_width"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "species"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "species",
        "value_set": ["setosa", "versicolor", "virginica"]
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sepal_length",
        "min_value": 3.5,
        "max_value": 8.5
      },
      "meta": {
        "notes": "Valores mais restritivos após limpeza"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sepal_width",
        "min_value": 1.5,
        "max_value": 5.0
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "petal_length",
        "min_value": 0.8,
        "max_value": 7.5
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "petal_width", 
        "min_value": 0.2,
        "max_value": 3.0
      }
    },
    {
      "expectation_type": "expect_column_mean_to_be_between",
      "kwargs": {
        "column": "sepal_length",
        "min_value": 5.0,
        "max_value": 6.5
      },
      "meta": {
        "notes": "Validação estatística - média esperada do dataset Iris"
      }
    },
    {
      "expectation_type": "expect_column_stdev_to_be_between",
      "kwargs": {
        "column": "sepal_length",
        "min_value": 0.5,
        "max_value": 1.5
      }
    }
  ],
  "meta": {
    "great_expectations_version": "0.15.0",
